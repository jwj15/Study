<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Test1"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".CardViewActivity"
            android:label="@string/title_activity_card_view"
            android:theme="@style/Theme.Test1.NoActionBar"></activity>
        <activity
            android:name=".ToolbarActivity"
            android:label="Toolbar Test"
            android:theme="@style/Theme.Test1.NoActionBar" />
        <activity
            android:name=".AsyncTest"
            android:label="AsyncTest" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".ScrollingActivity"
            android:label="Adapter Test!!" />
        <activity
            android:name=".WebViewActivity"
            android:label="WebView Test" />
        <activity
            android:name=".NaviActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".DisplayMessageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>